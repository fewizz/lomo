{

"nameKey": "pipeline.lomo.name",
"descriptionKey": "pipeline.lomo.desc",

"glslVersion": 400,
"enablePBR": true,

"materialProgram": {
	"vertexSource": "lomo:shaders/pipeline/lomo.vert",
	"fragmentSource": "lomo:shaders/pipeline/lomo.frag"
},

"drawTargets": {
	"solidTerrain": "solid_cnged",
	"translucentTerrain": "translucent_cnged",
	"translucentEntity": "entity_cnged",
	"weather": "weather_cnged",
	"clouds": "cloud_cnged",
	"translucentParticles": "particle_cnged"
},

"sky": {
	"defaultZenithAngle": -20
},

"fabulousTargets": {
	"entity": "entity_cnged",
	"particles": "particle_cnged",
	"weather": "weather_cnged",
	"clouds": "cloud_cnged",
	"translucent": "translucent_cnged"
},

"defaultFramebuffer": "solid_cd",

"skyShadows": {
	"framebuffer": "shadow_map",
	"vertexSource": "canvas:shaders/pipeline/shadow.vert",
	"fragmentSource": "canvas:shaders/pipeline/shadow.frag",
	"offsetBiasUnits": 4.0,
	"offsetSlopeFactor": 2.0
},
"options": [
	{
		"includeToken": "lomo:general",
		"categoryKey": "config.lomo.category.general",
		"options": {
			"reflections": {
				"choices": [ "all", "shiny", "water", "none" ],
				"enum": true,
				"prefix": "reflections_",
				"nameKey": "config.lomo.value.reflections",
				"descriptionKey": "config.lomo.help.reflections"
			},
			"tonemapping": {
				"choices": [ "exposure_1", "aces", "none" ],
				"enum": true,
				"prefix": "tonemapping_",
				"nameKey": "config.lomo.value.tonemapping",
				"descriptionKey": "config.lomo.help.tonemapping"
			}
		}
	},
	{
		"includeToken": "canvas:bloom_config",
		"categoryKey": "config.canvas.category.bloom",
		"options": {
			"bloom_toggle": {
				"default": true,
				"nameKey": "config.canvas.value.bloom_toggle",
				"descriptionKey": "config.canvas.help.bloom_toggle"
			},
			"bloom_intensity": {
				"default": 0.1,
				"min": 0.01,
				"max": 0.8,
				"nameKey": "config.canvas.value.bloom_intensity",
				"descriptionKey": "config.canvas.help.bloom_intensity"
			},
			"bloom_downsample_dist": {
				"default": 1.0,
				"min": 0.1,
				"max": 2.0,
				"nameKey": "config.canvas.value.bloom_downsample_scale",
				"descriptionKey": "config.canvas.help.bloom_downsample_scale"
			},
			"bloom_upsample_dist": {
				"default": 0.1,
				"min": 0.02,
				"max": 1.0,
				"nameKey": "config.canvas.value.bloom_upsample_scale",
				"descriptionKey": "config.canvas.help.bloom_upsample_scale"
			}
		}
	}
],
"programs": [
	{
		"name": "downsample",
		"vertexSource": "canvas:shaders/pipeline/post/simple_full_frame.vert",
		"fragmentSource": "canvas:shaders/pipeline/post/downsample.frag",
		"samplers": ["_cvu_input"]
	},
	{
		"name": "upsample_first",
		"vertexSource": "canvas:shaders/pipeline/post/simple_full_frame.vert",
		"fragmentSource": "canvas:shaders/pipeline/post/upsample_first.frag",
		"samplers": ["_cvu_input"]
	},
	{
		"name": "upsample",
		"vertexSource": "canvas:shaders/pipeline/post/simple_full_frame.vert",
		"fragmentSource": "canvas:shaders/pipeline/post/upsample.frag",
		"samplers": ["_cvu_input", "cvu_prior"]
	},
	{
		"name": "copy",
		"vertexSource": "lomo:shaders/pipeline/full_frame_depth_1.vert",
		"fragmentSource": "lomo:shaders/pipeline/copy.frag",
		"samplers": [ "u_input" ]
	},
	{
		"name": "post_1",
		"vertexSource": "lomo:shaders/pipeline/full_frame_depth_1.vert",
		"fragmentSource": "lomo:shaders/pipeline/post_1.frag",
		"samplers": [
			"u_color", "u_normal",
			"u_extra_0", "u_extra_1",
			"u_depth",
			"u_win_normal", "u_hi_depth",
			"u_prev_light_1_accum",
			"u_taa",
			"u_prev_depth",
			"u_shadow_map"
		]
	},
	{
		"name": "post_0",
		"vertexSource": "lomo:shaders/pipeline/full_frame_depth_1.vert",
		"fragmentSource": "lomo:shaders/pipeline/post_0.frag",
		"samplers": [
			"u_normal",
			"u_color",
			"u_depth",
			"u_extra_0",
			"u_extra_1",
			"u_light_1_accum",
			"u_prev_taa",
			"u_shadow_map"
		]
	},
	{
		"name": "copy_with_depth",
		"vertexSource": "lomo:shaders/pipeline/full_frame_depth_1.vert",
		"fragmentSource": "lomo:shaders/pipeline/copy_with_depth.frag",
		"samplers": [ "u_input", "u_input_depth" ]
	},
	{
		"name": "indexed_sort",
		"vertexSource": "lomo:shaders/pipeline/full_frame_depth_1.vert",
		"fragmentSource": "lomo:shaders/pipeline/indexed_sort.frag",
		"samplers": [
			"u_solid_d",
			"u_translucent_d",
			"u_entity_d",
			"u_particle_d",
			"u_weather_d"
		]
	},
	{
		"name": "select_emissive",
		"vertexSource": "lomo:shaders/pipeline/full_frame_depth_1.vert",
		"fragmentSource": "lomo:shaders/pipeline/select_emissive.frag",
		"samplers": [ "u_light" ]
	},
	{
		"name": "depth_levels",
		"vertexSource": "lomo:shaders/pipeline/full_frame_depth_1.vert",
		"fragmentSource": "lomo:shaders/pipeline/depth_levels.frag",
		"samplers": [ "u_depths" ]
	},
	{
		"name": "sort",
		"vertexSource": "lomo:shaders/pipeline/full_frame_depth_1.vert",
		"fragmentSource": "lomo:shaders/pipeline/sort.frag",
		"samplers": [
			"u_solid", "u_translucent", "u_entity",
			"u_particle", "u_weather",
			"u_index_to_type"
		]
	},
	{
		"name": "sort_colors",
		"vertexSource": "lomo:shaders/pipeline/full_frame_depth_1.vert",
		"fragmentSource": "lomo:shaders/pipeline/sort_colors.frag",
		"samplers": [
			"u_solid_c", "u_translucent_c", "u_entity_c",
			"u_particle_c", "u_weather_c",
			"u_index_to_type"
		]
	},
	{
		"name": "win_normals",
		"vertexSource": "lomo:shaders/pipeline/full_frame_depth_1.vert",
		"fragmentSource": "lomo:shaders/pipeline/win_normals.frag",
		"samplers": [
			"u_depths", "u_normals"
		]
	},
	{
		"name": "normal_from_depth",
		"vertexSource": "lomo:shaders/pipeline/full_frame_depth_1.vert",
		"fragmentSource": "lomo:shaders/pipeline/normal_from_depth.frag",
		"samplers": [
			"u_depth"
		]
	},
	{
		"name": "extra_1_1_0",
		"vertexSource": "lomo:shaders/pipeline/full_frame_depth_1.vert",
		"fragmentSource": "lomo:shaders/pipeline/extra_1_1_0.frag",
		"samplers": []
	},
	{
		"name": "extra_0_1_0",
		"vertexSource": "lomo:shaders/pipeline/full_frame_depth_1.vert",
		"fragmentSource": "lomo:shaders/pipeline/extra_0_1_0.frag",
		"samplers": []
	},
	{
		"name": "combine",
		"vertexSource": "lomo:shaders/pipeline/full_frame_depth_1.vert",
		"fragmentSource": "lomo:shaders/pipeline/combine.frag",
		"samplers": [ "u_colors" ]
	},
	{
		"name": "merge_bloom",
		"vertexSource": "lomo:shaders/pipeline/full_frame_depth_1.vert",
		"fragmentSource": "lomo:shaders/pipeline/merge_bloom.frag",
		"samplers": [ "u_bloom", "u_light" ]
	}
],

"beforeWorldRender": {
	"passes": [
		{ "name": "clear_solid", "framebuffer": "solid_cnged", "program": "frex_clear" },
		{ "name": "clear_entity", "framebuffer": "entity_cnged", "program": "frex_clear" },
		{ "name": "clear_translucent", "framebuffer": "translucent_cnged", "program": "frex_clear" },
		{ "name": "clear_weather", "framebuffer": "weather_cnged", "program": "frex_clear" },
		{ "name": "clear_cloud", "framebuffer": "cloud_cnged", "program": "frex_clear" },
		{ "name": "clear_particle", "framebuffer": "particle_cnged", "program": "frex_clear" },
		{
			"name": "solid_extra_1_1_0",
			"program": "extra_1_1_0",
			"framebuffer": "solid_e_0",
			"samplerImages": []
		}
	]
},

"fabulous": {
	"passes": [
		{
			"name": "cloud_normal_from_depth",
			"program": "normal_from_depth",
			"framebuffer": "cloud_n",
			"samplerImages": [ "cloud_d" ]
		},
		{
			"name": "copy_cloud_normal",
			"program": "copy",
			"framebuffer": "cloud_g",
			"samplerImages": [ "cloud_n" ]
		},
		{
			"name": "cloud_extra_1_1_0",
			"program": "extra_1_1_0",
			"framebuffer": "cloud_e_0",
			"samplerImages": []
		},
		{
			"name": "weather_normal_from_depth",
			"program": "normal_from_depth",
			"framebuffer": "weather_n",
			"samplerImages": [ "weather_d" ]
		},
		{
			"name": "copy_weather_normal",
			"program": "copy",
			"framebuffer": "weather_g",
			"samplerImages": [ "weather_n" ]
		},
		{
			"name": "weather_extra_0_1_0",
			"program": "extra_0_1_0",
			"framebuffer": "weather_e_0",
			"samplerImages": []
		},
		{
			"name": "indexed_sort",
			"program": "indexed_sort",
			"framebuffer": "depth_indices",
			"samplerImages": [
				"solid_d", "translucent_d",
				"entity_d", "particle_d", "weather_d"
			]
		},
		{
			"name": "sort_colors",
			"program": "sort_colors",
			"framebuffer": "colors",
			"samplerImages": [
				"solid_c", "translucent_c", "entity_c",
				"particle_c", "weather_c", "index_to_type"
			]
		},
		{
			"name": "sort_normals",
			"program": "sort",
			"framebuffer": "normals",
			"samplerImages": [
				"solid_n", "translucent_n", "entity_n",
				"particle_n", "weather_n", "index_to_type"
			]
		},
		{
			"name": "sort_geometric_normals",
			"program": "sort",
			"framebuffer": "geometric_normals",
			"samplerImages": [
				"solid_g", "translucent_g", "entity_g",
				"particle_g", "weather_g", "index_to_type"
			]
		},
		{
			"name": "sort_extras_0",
			"program": "sort",
			"framebuffer": "extras_0",
			"samplerImages": [
				"solid_e_0", "translucent_e_0", "entity_e_0",
				"particle_e_0", "weather_e_0", "index_to_type"
			]
		},
		{
			"name": "sort_extras_1",
			"program": "sort",
			"framebuffer": "extras_1",
			"samplerImages": [
				"solid_e_1", "translucent_e_1", "entity_e_1",
				"particle_e_1", "weather_e_1", "index_to_type"
			]
		},
		{
			"name": "sort_depths",
			"program": "sort",
			"framebuffer": "depths",
			"samplerImages": [
				"solid_d", "translucent_d", "entity_d",
				"particle_d", "weather_d", "index_to_type"
			]
		},
		{
			"name": "win_normals",
			"program": "win_normals",
			"framebuffer": "win_normals_hi_depths_0",
			"samplerImages": [
				"depths", "geometric_normals"
			]
		},
		{
			"name": "hi_depths_1",
			"program": "depth_levels",
			"framebuffer": "hi_depths_1",
			"samplerImages": [ "hi_depths" ],
			"lod": 1
		},
		{
			"name": "hi_depths_2",
			"program": "depth_levels",
			"framebuffer": "hi_depths_2",
			"samplerImages": [ "hi_depths" ],
			"lod": 2
		},
		{
			"name": "hi_depths_3",
			"program": "depth_levels",
			"framebuffer": "hi_depths_3",
			"samplerImages": [ "hi_depths" ],
			"lod": 3
		},
		{
			"name": "hi_depths_4",
			"program": "depth_levels",
			"framebuffer": "hi_depths_4",
			"samplerImages": [ "hi_depths" ],
			"lod": 4
		},
		{
			"name": "post_1",
			"program": "post_1",
			"framebuffer": "light_1_accum__prev_taa",
			"samplerImages": [
				"colors",
				"normals",
				"extras_0",
				"extras_1",
				"depths",
				"win_normals",
				"hi_depths",
				"prev_light_1_accum",
				"taa",
				"prev_depth",
				"shadow_map"
			]
		},
		{
			"name": "post_0",
			"program": "post_0",
			"framebuffer": "prev_light_1_accum__prev_depth__light__taa",
			"samplerImages": [
				"normals",
				"colors",
				"depths",
				"extras_0",
				"extras_1",
				"light_1_accum",
				"prev_taa",
				"shadow_map"
			]
		},
		{
			"name": "select_emissive",
			"program": "select_emissive",
			"framebuffer": "emissive",
			"samplerImages": [
				"light"
			]
		},
		{
			"name": "bloom_downsample_0",
			"program": "downsample",
			"framebuffer": "bloom_downsample_0",
			"samplerImages": ["emissive"],
			"toggleConfig": "bloom_toggle"
		},
		{
			"name": "bloom_downsample_1",
			"program": "downsample",
			"framebuffer": "bloom_downsample_1",
			"samplerImages": ["bloom_downsample"],
			"lod": 1,
			"toggleConfig": "bloom_toggle"
		},
		{
			"name": "bloom_downsample_2",
			"program": "downsample",
			"framebuffer": "bloom_downsample_2",
			"samplerImages": ["bloom_downsample"],
			"lod": 2,
			"toggleConfig": "bloom_toggle"
		},
		{
			"name": "bloom_downsample_3",
			"program": "downsample",
			"framebuffer": "bloom_downsample_3",
			"samplerImages": ["bloom_downsample"],
			"lod": 3,
			"toggleConfig": "bloom_toggle"
		},
		{
			"name": "bloom_downsample_4",
			"program": "downsample",
			"framebuffer": "bloom_downsample_4",
			"samplerImages": ["bloom_downsample"],
			"lod": 4,
			"toggleConfig": "bloom_toggle"
		},
		{
			"name": "bloom_downsample_5",
			"program": "downsample",
			"framebuffer": "bloom_downsample_5",
			"samplerImages": ["bloom_downsample"],
			"lod": 5,
			"toggleConfig": "bloom_toggle"
		},
		{
			"name": "bloom_downsample_6",
			"program": "downsample",
			"framebuffer": "bloom_downsample_6",
			"samplerImages": ["bloom_downsample"],
			"lod": 6,
			"toggleConfig": "bloom_toggle"
		},
		{
			"name": "bloom_upsample_6",
			"program": "upsample_first",
			"framebuffer": "bloom_upsample_6",
			"samplerImages": ["bloom_downsample"],
			"lod": 6,
			"toggleConfig": "bloom_toggle"
		},
		{
			"name": "bloom_upsample_5",
			"program": "upsample",
			"framebuffer": "bloom_upsample_5",
			"samplerImages": ["bloom_downsample", "bloom_upsample"],
			"lod": 5,
			"toggleConfig": "bloom_toggle"
		},
		{
			"name": "bloom_upsample_4",
			"program": "upsample",
			"framebuffer": "bloom_upsample_4",
			"samplerImages": ["bloom_downsample", "bloom_upsample"],
			"lod": 4,
			"toggleConfig": "bloom_toggle"
		},
		{
			"name": "bloom_upsample_3",
			"program": "upsample",
			"framebuffer": "bloom_upsample_3",
			"samplerImages": ["bloom_downsample", "bloom_upsample"],
			"lod": 3,
			"toggleConfig": "bloom_toggle"
		},
		{
			"name": "bloom_upsample_2",
			"program": "upsample",
			"framebuffer": "bloom_upsample_2",
			"samplerImages": ["bloom_downsample", "bloom_upsample"],
			"lod": 2,
			"toggleConfig": "bloom_toggle"
		},
		{
			"name": "bloom_upsample_1",
			"program": "upsample",
			"framebuffer": "bloom_upsample_1",
			"samplerImages": ["bloom_downsample", "bloom_upsample"],
			"lod": 1,
			"toggleConfig": "bloom_toggle"
		},
		{
			"name": "bloom_upsample_0",
			"program": "upsample",
			"framebuffer": "bloom_upsample_0",
			"samplerImages": ["bloom_downsample", "bloom_upsample"],
			"lod": 0,
			"toggleConfig": "bloom_toggle"
		},
		{
			"name": "merge_bloom",
			"program": "merge_bloom",
			"framebuffer": "solid_c",
			"samplerImages": ["bloom_upsample", "light"]
		}
	]
},

