{

programs: [
	{
		name: "generate_data",
		vertexSource: "lomo:shaders/full_frame.vert",
		fragmentSource: "lomo:shaders/fabulous/generate_data.frag",
		samplers: [ "u_depth" ],
	}
],

fabulous: { passes: [
	{
		name: "generate_weather_data",
		program: "generate_data",
		framebuffer: "weather_data",
		samplerImages: [ "weather_depth" ]
	},
	/*{
		name: "generate_misc_normal",
		program: "generate_normal",
		framebuffer: "misc_normal",
		samplerImages: [ "misc_depth" ]
	}*/
]},

framebuffers: [
	{
		name: "weather_data",
		colorAttachments: [
			{ image: "weather_data" }
		],
	},
	{
		name: "misc_normal",
		colorAttachments: [
			{ image: "misc_data" }
		],
	}
]

}