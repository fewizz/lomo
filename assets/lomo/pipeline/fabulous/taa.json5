{

programs: [
	{
		name: "taa",
		vertexSource: "lomo:shaders/full_frame.vert",
		fragmentSource: "lomo:shaders/fabulous/taa.frag",
		samplers: [
			"u_color", "u_depth",
			"u_previous_taa"
		],
	}
],

onResize: { passes: [
	{
		name: "clear_previous_taa",
		framebuffer: "previous_taa",
		program: "frex_clear"
	}
]},

fabulous: { passes: [
	{
		name: "taa",
		framebuffer: "taa",
		program: "taa",
		samplerImages: [
			"post", "blended_depth",
			"previous_taa"
		]
	},
	{
		name: "copy_taa",
		framebuffer: "previous_taa",
		program: "copy",
		samplerImages: [ "taa" ]
	},
	{
		name: "copy_taa_to_main",
		framebuffer: "solid",
		program: "copy",
		samplerImages: [ "taa" ]
	}
]},

images: [
	{
		name: "taa",
		internalFormat: "RGBA16F",
		pixelFormat: "RGBA",
		texParams: [
			{ name: "TEXTURE_MIN_FILTER", val: "NEAREST" },
			{ name: "TEXTURE_MAG_FILTER", val: "NEAREST" },
			{ name: "TEXTURE_WRAP_S", val: "CLAMP_TO_EDGE" },
			{ name: "TEXTURE_WRAP_T", val: "CLAMP_TO_EDGE" }
		]
	},
	{
		name: "previous_taa",
		internalFormat: "RGBA16F",
		pixelFormat: "RGBA",
		texParams: [
			{ name: "TEXTURE_MIN_FILTER", val: "LINEAR" },
			{ name: "TEXTURE_MAG_FILTER", val: "LINEAR" },
			{ name: "TEXTURE_WRAP_S", val: "CLAMP_TO_EDGE" },
			{ name: "TEXTURE_WRAP_T", val: "CLAMP_TO_EDGE" }
		]
	}
],

framebuffers: [
	{
		name: "taa",
		colorAttachments: [{
			image: "taa"
		}]
	},
	{
		name: "previous_taa",
		colorAttachments: [{
			image: "previous_taa", clearColor: 0
		}]
	}
]

}