{

programs: [
	{
		name: "post",
		vertexSource: "lomo:shaders/full_frame.vert",
		fragmentSource: "lomo:shaders/fabulous/post.frag",
		samplers: [
			"u_blended_without_particles_color", "u_blended_without_particles_depth",
			"u_blended_color", "u_blended_normal", "u_blended_data", "u_blended_depth",
			"u_solid_color", "u_solid_depth", "u_translucent_depth", "u_translucent_color"
			"u_skybox"
		]
	}
],

fabulous: { passes: [
	{
		name: "post",
		program: "post",
		framebuffer: "post",
		samplerImages: [
			"blended_without_particles_color", "blended_without_particles_depth",
			"blended_color", "blended_normal", "blended_data", "blended_depth",
			"solid_color", "solid_depth", "translucent_depth", "translucent_color",
			"skybox"
		]
	}
]},

images: [
	{
		name: "post",
		internalFormat: "RGBA16F",
		pixelFormat: "RGBA",
		texParams: [
			{ name: "TEXTURE_MIN_FILTER", val: "NEAREST" },
			{ name: "TEXTURE_MAG_FILTER", val: "NEAREST" },
			{ name: "TEXTURE_WRAP_S", val: "CLAMP_TO_EDGE" },
			{ name: "TEXTURE_WRAP_T", val: "CLAMP_TO_EDGE" }
		]
	}
],

framebuffers: [
	{
		name: "post",
		colorAttachments: [{
			image: "post"
		}]
	}
]

}