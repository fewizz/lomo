		},
		{
			"name": "depths_1",
			"program": "depth_levels",
			"framebuffer": "depths_1",
			"samplerImages": [ "depths" ],
			"lod": 1
		},
		{
			"name": "depths_2",
			"program": "depth_levels",
			"framebuffer": "depths_2",
			"samplerImages": [ "depths" ],
			"lod": 2
		},
		{
			"name": "depths_3",
			"program": "depth_levels",
			"framebuffer": "depths_3",
			"samplerImages": [ "depths" ],
			"lod": 3
		},
		{
			"name": "depths_4",
			"program": "depth_levels",
			"framebuffer": "depths_4",
			"samplerImages": [ "depths" ],
			"lod": 4
		},
		{
			"name": "post",
			"program": "post",
			"framebuffer": "solid_c",
			"samplerImages": [
				"colors",
				"normals",
				"extras",
				"depths"
			]
		}
	]
