{

"nameKey": "pipeline.lomo.name",
"descriptionKey": "pipeline.canvas_basic.desc",
"enablePBR": true,

"include": [
	"canvas:pipeline/basic_light_config.json",
	"canvas:pipeline/fog_config.json",
	"canvas:pipeline/handheld_light_config.json"
],

"materialProgram": {
	"vertexSource": "lomo:shaders/pipeline/lomo.vert",
	"fragmentSource": "lomo:shaders/pipeline/lomo.frag"
},

"drawTargets": {
	"solidTerrain": "solid_cned",
	"translucentTerrain": "translucent_cned",
	"translucentEntity": "entity_cned",
	"weather": "weather_cned",
	"clouds": "cloud_cd",
	"translucentParticles": "particle_cned"
},

"fabulousTargets": {
	"entity": "entity_cned",
	"particles": "particle_cned",
	"weather": "weather_cned",
	"clouds": "cloud_cd",
	"translucent": "translucent_cned"
},

"defaultFramebuffer": "solid_cd",

"programs": [
	{
		"name": "sky",
		"vertexSource": "lomo:shaders/pipeline/full_frame_depth_1.vert",
		"fragmentSource": "lomo:shaders/pipeline/sky.frag",
		"samplers": [ "u_solid_d" ]
	},
	{
		"name": "copy",
		"vertexSource": "lomo:shaders/pipeline/full_frame_depth_1.vert",
		"fragmentSource": "lomo:shaders/pipeline/copy.frag",
		"samplers": [ "u_input" ]
	},
	{
		"name": "copy_with_depth",
		"vertexSource": "lomo:shaders/pipeline/full_frame_depth_1.vert",
		"fragmentSource": "lomo:shaders/pipeline/copy_with_depth.frag",
		"samplers": [ "u_input", "u_input_depth" ]
	},
	{
		"name": "indexed_sort",
		"vertexSource": "lomo:shaders/pipeline/full_frame_depth_1.vert",
		"fragmentSource": "lomo:shaders/pipeline/indexed_sort.frag",
		"samplers": [ "u_d0", "u_d1", "u_d2", "u_d3", "u_d4", "u_d5" ]
	},
	{
		"name": "sort",
		"vertexSource": "lomo:shaders/pipeline/full_frame_depth_1.vert",
		"fragmentSource": "lomo:shaders/pipeline/sort.frag",
		"samplers": [
			"u_solid_c", "u_solid_n", "u_solid_d", "u_solid_e",
			"u_translucent_c", "u_translucent_n", "u_translucent_d", "u_translucent_e",
			"u_entity_c", "u_entity_n", "u_entity_d", "u_entity_e",
			"u_weather_c", "u_weather_n", "u_weather_d", "u_weather_e",
			"u_cloud_c", "u_cloud_d",
			"u_particle_c", "u_particle_n", "u_particle_d", "u_particle_e"
		]
	},
	{
		"name": "copy_2_depth",
		"vertexSource": "lomo:shaders/pipeline/full_frame_depth_1.vert",
		"fragmentSource": "lomo:shaders/pipeline/copy_2_depth.frag",
		"samplers": [ "u_depth_0", "u_depth_1" ]
	},
	{
		"name": "depth_level_4",
		"vertexSource": "lomo:shaders/pipeline/full_frame_depth_1.vert",
		"fragmentSource": "lomo:shaders/pipeline/depth_level_4.frag",
		"samplers": [ "u_depth_0", "u_depth_1" ]
	},
	{
		"name": "post",
		"vertexSource": "lomo:shaders/pipeline/full_frame_depth_1.vert",
		"fragmentSource": "lomo:shaders/pipeline/post.frag",
		"samplers": [
			"u_sorted_without_translucent_c",
			"u_sorted_without_translucent_n",
			"u_sorted_without_translucent_d",
			"u_sorted_with_translucent_c",
			"u_sorted_with_translucent_n",
			"u_sorted_with_translucent_d",
			"u_sorted_all_c",
			"u_translucent_c"
		]
	}
],

"fabulous": {
	"passes": [
		{
			"name": "sky",
			"program" : "sky",
			"framebuffer": "solid_c",
			"samplerImages": [ "solid_d" ]
		},
		{
			"name": "indexed_sort",
			"program": "indexed_sort",
			"framebuffer": "depth_indices",
			"samplerImages": [
				"solid_d", "translucent_d", "entity_d",
				"weather_d", "cloud_d", "particle_d"
			]
		}/*,
		{
			"name": "sort",
			"program": "sort",
			"framebuffer": "sorted_cned",
			"samplerImages": [
				"solid_c", "solid_n", "solid_d", "solid_e",
				"translucent_c", "translucent_n", "translucent_d", "translucent_e",
				"entity_c", "entity_n", "entity_d", "entity_e",
				"weather_c", "weather_n", "weather_d", "weather_e",
				"cloud_c", "cloud_d",
				"particle_c", "particle_n", "particle_d", "particle_e"
			]
		},
		{
			"name": "level_0",
			"program": "copy_2_depth",
			"framebuffer": "level_0_d",
			"samplerImages": [ "sorted_with_translucent_d", "sorted_without_translucent_d" ],
			"lod": 0
		},
		{
			"name": "level_1",
			"program": "depth_level_4",
			"framebuffer": "level_1_d",
			"samplerImages": [ "level_d", "solid_level_d" ],
			"lod": 1
		},
		{
			"name": "level_2",
			"program": "depth_level_4",
			"framebuffer": "level_2_d",
			"samplerImages": [ "level_d", "solid_level_d" ],
			"lod": 2
		},
		{
			"name": "level_3",
			"program": "depth_level_4",
			"framebuffer": "level_3_d",
			"samplerImages": [ "level_d", "solid_level_d" ],
			"lod": 3
		},
		{
			"name": "level_4",
			"program": "depth_level_4",
			"framebuffer": "level_4_d",
			"samplerImages": [ "level_d", "solid_level_d" ],
			"lod": 4
		},
		{
			"name": "post",
			"program": "post",
			"framebuffer": "solid_c",
			"samplerImages": [
				"sorted_without_translucent_c",
				"sorted_without_translucent_n",
				"solid_level_d",
				"sorted_with_translucent_c",
				"sorted_with_translucent_n",
				"level_d",
				"sorted_all_c",
				"translucent_c"
			]
		}*/
		/*{
			"name": "copy_result",
			"program": "copy",
			"framebuffer": "solid",
			"samplerImages": [
				"post_result"
			]
		}*/
	]
},

