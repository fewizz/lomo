		"vertexSource": "lomo:shaders/pipeline/full_frame_depth_1.vert",
		"fragmentSource": "lomo:shaders/pipeline/post.frag",
		"samplers": [
			"u_colors", "u_normals", "u_extras", "u_depths"
		]
	},
	{
		"name": "copy_to_array_sorted",
		"vertexSource": "lomo:shaders/pipeline/full_frame_depth_1.vert",
		"fragmentSource": "lomo:shaders/pipeline/copy_to_array_sorted.frag",
		"samplers": [
			"u_i0",
			"u_i1",
			"u_i2",
			"u_i3",
			"u_i4",
			"u_i5",
			"u_index_to_type"
