"fabulous": {
	"passes": [
		{
			"name": "indexed_sort",
			"program": "indexed_sort",
			"framebuffer": "depth_indices",
			"samplerImages": [
				"solid_d", "translucent_d", "entity_d",
				"particle_d", "weather_d", "cloud_d"
			]
		},
		{
			"name": "sort_colors",
			"program": "copy_to_array_sorted_and_blend",
			"framebuffer": "colors",
			"samplerImages": [
				"solid_c", "translucent_c", "entity_c",
				"particle_c", "weather_c", "cloud_c", "index_to_type"
			]
		},
		{
			"name": "sort_normals",
			"program": "copy_to_array_sorted",
			"framebuffer": "normals",
			"samplerImages": [
				"solid_n", "translucent_n", "entity_n",
				"particle_n", "weather_n", "cloud_n", "index_to_type"
