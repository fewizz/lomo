{
"materialVertexShader": "lomo:shaders/pipeline/lomo.vert",
"materialFragmentShader": "lomo:shaders/pipeline/lomo.frag",

"drawTargets": {
	"solidTerrain": "solid",
	"translucentTerrain": "translucent",
	"translucentEntity": "entity",
	"weather": "weather",
	"clouds": "clouds",
	"translucentParticles": "particles"
},

"fabulousTargets": {
	"entity": "entity",
	"particles": "particles",
	"weather": "weather",
	"clouds": "clouds",
	"translucent": "translucent"
},

"programs": [
	{
		
		"name": "copy",
		"vertexSource": "lomo:shaders/pipeline/simple_full_frame_depth_one.vert",
		"fragmentSource": "canvas:shaders/pipeline/post/copy.frag",
		"samplers": ["_cvu_input"]
	},
	{
		
		"name": "copy_with_depth",
		"vertexSource": "lomo:shaders/pipeline/simple_full_frame_depth_one.vert",
		"fragmentSource": "lomo:shaders/pipeline/copy_with_depth.frag",
		"samplers": ["u_input", "u_input_depth"]
	},
	{
		"name": "fabulous",
		"vertexSource": "lomo:shaders/pipeline/simple_full_frame_depth_one.vert",
		"fragmentSource": "lomo:shaders/pipeline/fabulous_sort.frag",
		"samplers": [
			"solid_main", "solid_depth",
			"translucent_main", "translucent_depth",
			"entity_main", "entity_depth",
			"particles_main", "particles_depth",
			"weather_main", "weather_depth",
			"clouds_main", "clouds_depth"
		]
	},
	{
		"name": "depth_level",
		"vertexSource": "lomo:shaders/pipeline/simple_full_frame_depth_one.vert",
		"fragmentSource": "lomo:shaders/pipeline/depth_level.frag",
		"samplers": [ "u_depth" ]
	},
	{
		"name": "post",
		"vertexSource": "lomo:shaders/pipeline/simple_full_frame_depth_one.vert",
		"fragmentSource": "lomo:shaders/pipeline/post.frag",
		"samplers": [ "u_reflective", "u_main", "u_depth" ]
	}
],

"fabulous": {
	"passes": [
		{
			"name": "composite",
			"program": "fabulous",
			"framebuffer": "composite_0",
			"samplerImages": [
				"solid_main", "solid_depth",
				"translucent_main", "translucent_depth",
				"entity_main", "entity_depth",
				"particles_main", "particles_depth",
				"weather_main", "weather_depth",
				"clouds_main", "clouds_depth"
			]
		},
		{
			"name": "depth_0",
			"program": "copy",
			"framebuffer": "depth_level_0",
			"samplerImages": [ "composite_depth_0" ]
		},
		{
			"name": "depth_1",
			"program": "depth_level",
			"framebuffer": "depth_level_1",
			"samplerImages": [ "depth_levels" ],
			"lod": 1
		},
		{
			"name": "depth_2",
			"program": "depth_level",
			"framebuffer": "depth_level_2",
			"samplerImages": [ "depth_levels" ],
			"lod": 2
		},
		{
			"name": "depth_3",
			"program": "depth_level",
			"framebuffer": "depth_level_3",
			"samplerImages": [ "depth_levels" ],
			"lod": 3
		},
		{
			"name": "depth_4",
			"program": "depth_level",
			"framebuffer": "depth_level_4",
			"samplerImages": [ "depth_levels" ],
			"lod": 4
		},
		/*{
			"name": "depth_5",
			"program": "depth_level",
			"framebuffer": "depth_level_5",
			"samplerImages": [ "depth_levels" ],
			"lod": 5
		},
		{
			"name": "depth_6",
			"program": "depth_level",
			"framebuffer": "depth_level_6",
			"samplerImages": [ "depth_levels" ],
			"lod": 6
		},
		{
			"name": "depth_7",
			"program": "depth_level",
			"framebuffer": "depth_level_7",
			"samplerImages": [ "depth_levels" ],
			"lod": 7
		},
		{
			"name": "depth_8",
			"program": "depth_level",
			"framebuffer": "depth_level_8",
			"samplerImages": [ "depth_levels" ],
			"lod": 8
		},*/
		{
			"name": "post",
			"program": "post",
			"framebuffer": "composite_1",
			"samplerImages": [ "normal", "composite_main_0", "depth_levels" ]
		},
		{
			"name": "copy",
			"program": "copy",
			"framebuffer": "solid",
			"samplerImages": [ "composite_main_1" ]
		}
	]
},

